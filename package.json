  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@tailwindcss/postcss": "^4.1.9",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.0",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^38.2.2",
    "electron-builder": "^24.13.3",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^4.1.9",
    "ts-loader": "^9.5.1",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "AdhamFouadHussein",
        "repo": "electron-whatsapp",
        "private": false
      }
    ],
    "appId": "com.adhamfouad.whatsapp-manager",
    "productName": "WhatsApp Manager",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "src/backend/**/*",
      "package.json",
      "!node_modules/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    },
    "extraResources": [
      {
        "from": ".env.example",
        "to": ".env.example"
      },
      {
        "from": ".env.production",
        "to": ".env"
      }
    ]
  }
}