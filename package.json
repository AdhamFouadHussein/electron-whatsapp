{
  "name": "af-whatsapp-reminders",
  "version": "1.1.0",
  "description": "Professional Multi-Device WhatsApp Automation Platform | Event Management & Intelligent Reminder System with Multi-Language Support, Dark Mode UI, and Enterprise-Grade MySQL Synchronization",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run watch\" \"electron .\"",
    "watch": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win --x64",
    "dist:linux": "npm run build && electron-builder --linux",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Adham Fouad <adham@adhamfouad.com>",
  "license": "ISC",
